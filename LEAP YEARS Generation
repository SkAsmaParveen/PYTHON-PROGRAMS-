def LeapYears(y):
      if y % 4 == 0 and y % 100 != 0 or y % 400 == 0:
          return True
      else:
          return False
yr = int(input("enter the starting year:"))          
n=yr
leaps =[]
while len(leaps) != 15:
     yr.append(leaps)
     yr+=1

print("the leap years from the given year {n} are:" ,LeapYears(y))
